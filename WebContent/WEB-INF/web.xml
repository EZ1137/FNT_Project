<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>FNT_Project</display-name>
  <welcome-file-list>
    <welcome-file>fntmain.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>fntcontroller</servlet-name>
    <servlet-class>com.fnt.controller.FntController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fntcontroller</servlet-name>
    <url-pattern>/fnt.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>mypagecontroller</servlet-name>
  	<servlet-class>com.fnt.controller.MypageController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>mypagecontroller</servlet-name>
  	<url-pattern>/mypage.do</url-pattern>
  </servlet-mapping>
  
    <servlet>
  	<servlet-name>admincontroller</servlet-name>
  	<servlet-class>com.fnt.controller.AdminpageController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>admincontroller</servlet-name>
  	<url-pattern>/admin.do</url-pattern> 
  </servlet-mapping>
</web-app>