<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>FNT_Project</display-name>
  <welcome-file-list>
    <welcome-file>fntmain.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>noticecontroller</servlet-name>
    <servlet-class>com.fnt.controller.NoticeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>noticecontroller</servlet-name>
    <url-pattern>/notice.do</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>logioncrud</servlet-name>
    <servlet-class>com.fnt.controller.LogionCrudController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logioncrud</servlet-name>
    <url-pattern>/logincrud.do</url-pattern>
  </servlet-mapping>
  
  
</web-app>
